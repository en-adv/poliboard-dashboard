{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/v1/users", "dest": "/api/users.js" },
    { "src": "/api/v1/properties", "dest": "/api/properties.js" },
    { "src": "/api/v1/metrics", "dest": "/api/metrics.js" },
    { "src": "/api/v1/charts", "dest": "/api/charts.js" },
    { "src": "/api/v1/datacalc", "dest": "/api/datacalc.js" }
  ]
}
