{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/v1/users", "dest": "/api/users.js" },
    { "src": "/api/v1/properties", "dest": "/api/properties.js" },
    { "src": "/api/v1/metrics", "dest": "/api/metrics.js" }
  ]
}
